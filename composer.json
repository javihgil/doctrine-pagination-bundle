{
    "name": "javihgil/doctrine-pagination-bundle",
    "description": "Doctrine pagination bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jhg\\DoctrinePaginationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jhg\\DoctrinePaginationBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "javihgil/doctrine-pagination": "^2.0"
    },
    "require-dev": {
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/routing": "^5.4 || ^6.0",
        "twig/twig": "^2.12|^3.0",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    }
}
